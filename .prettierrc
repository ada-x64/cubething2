{
  "printWidth": 80,
  "tabWidth": 2,
  "semi": true,
  "proseWrap": "always",
  "singleAttributePerLine": true,
  "bracketSpacing": true,
  "plugins": ["prettier-plugin-latex"],
  "overrides": [
    {
      "files": ["*.cls", "*.sty"],
      "options": {
        "parser": "latex-parser"
      }
    }
  ]
}
